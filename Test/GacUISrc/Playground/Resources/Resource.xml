<Resource>
  <Instance name="ThemeResource">
    <Instance ref.CodeBehind="false" ref.Class="demo::TestTheme" xmlns:x="testtheme::*">
      <ThemeTemplates Name="TestSkin">
      </ThemeTemplates>
    </Instance>
  </Instance>

  <Folder name="Images" content="Link">../../../Resources/App/FullControlTest/ToolbarImages/Images.xml</Folder>

  <Instance name="MainWindowResource">
    <Instance ref.CodeBehind="false" ref.Class="demo::TestWindow">
      <Window ref.Name="self" Text="MainWindow" ClientSize="x:640 y:480">
        <Stack Direction="Vertical" Padding="5" AlignmentToParent="left:5 top:5 right:5 bottom:5" MinSizeLimitation="LimitToElementAndChildren">
          <StackItem MinSizeLimitation="LimitToElementAndChildren">
            <Table MinSizeLimitation="LimitToElementAndChildren" CellPadding="5" BorderVisible="false" AlignmentToParent="left:0 top:0 right:0 bottom:0">
              <att.Rows>
                <_>composeType:Absolute absolute:30</_>
              </att.Rows>
              <att.Columns>
                <_>composeType:Absolute absolute:200</_>
              </att.Columns>

              <Cell Site="row:0 column:0"><Label Text="Title:"/></Cell>
            </Table>
          </StackItem>
        </Stack>
      </Window>
    </Instance>
  </Instance>
</Resource>